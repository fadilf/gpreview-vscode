{
  "name": "gpreview",
  "displayName": "GPreview",
  "description": "Extension that let's you preview LabVIEW VI files in VS Code.",
  "version": "0.0.17",
  "publisher": "fadil",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/fadilf/gpreview-vscode.git"
  },
  "homepage": "https://github.com/fadilf/gpreview-vscode#readme",
  "bugs": {
    "url": "https://github.com/fadilf/gpreview-vscode/issues"
  },
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "gpreview.vi",
        "displayName": "GPreview VI",
        "selector": [
          { "filenamePattern": "*.vi" },
          { "filenamePattern": "*.vim" },
          { "filenamePattern": "*.vit" },
          { "filenamePattern": "*.ctl" }
        ],
        "priority": "default"
      }
    ],
    "configuration":{
      "title": "GPreview Settings",
      "type": "object",
      "properties": {
        "gpreview.viServerPort": {
          "type": "integer",
          "default": 3363,
          "description": "Specifies the LabVIEW VI server port to use"
        },
        "gpreview.labViewFilePath": {
          "type": "string",
          "default": "",
          "description": "Specifies the LabVIEW executable file path to use"
        },
        "gpreview.defaultTab": {
          "type": "string",
          "default": "Info",
          "enum": ["Info", "FP", "BD"],
          "description": "Specifies the tab that is open when the file is loaded. If the tab is unavailable, defaults to Info"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  }
}
